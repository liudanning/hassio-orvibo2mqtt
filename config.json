{
  "name": "Orvibo2mqtt",
  "version": "1.0.0",
  "slug": "orvibo2mqtt",
  "description": "Orvibo2mqtt add-on",
  "uart": true,
  "url": "https://github.com/liudanning/hassio-orvibo2mqtt",
  "startup": "application",
  "services": [
    "mqtt:need"
  ],
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "map": [
    "share:rw",
    "config:rw"
  ],
  "ports": {
    "10001/tcp": 10001
  },
  "ports_description": {
    "10001/tcp": "orvibo-b25-server tcp-listen port"
  },
  "options": {
    "config_path": "/config/orvibo2mqtt"
  },
  "schema": {
    "config_path": "str"
  }
}
